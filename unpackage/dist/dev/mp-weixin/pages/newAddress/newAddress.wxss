.newAddress {
  position: absolute;
  height: 100%;
  width: 100%;
  background-color: #F8F8F8;
}
.newAddress .header {
  box-sizing: border-box;
  padding: 36rpx 0 0 13rpx ;
  color: #222;
  background-color: #fff;
}
.newAddress .header .text {
  text-align: center;
  font-weight: 600;
  font-size: 32rpx;
  letter-spacing: 2rpx;
  margin-bottom: 30rpx;
}
.newAddress .header .bitian {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 2rpx solid #E3E1E1;
  border-top: 1rpx solid transparent;
  height: 84rpx;
}
.newAddress .header .bitian .xxx {
  opacity: 0;
}
.newAddress .header .bitian .two {
  color: #E83235;
  font-size: 12rpx;
  margin-top: 48rpx;
  margin-right: 4rpx;
}
.newAddress .header .bitian .tianxie {
  margin-top: 34rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
}
.newAddress .header .bitian .tianxie text {
  font-weight: 600;
  display: inline-block;
  margin-right: 90rpx;
  width: 120rpx;
}
.newAddress .header .bitian .tianxie .bitiantext {
  width: 400rpx;
  margin-right: 0rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.newAddress .header .menpai {
  border-bottom: none;
  height: 94rpx;
}
.newAddress .add {
  position: fixed;
  bottom: 0;
  width: 100%;
  letter-spacing: 2rpx;
  line-height: 98rpx;
  background-color: #E73437;
  color: #fff;
  font-size: 34rpx;
  text-align: center;
}

